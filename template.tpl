___SANDBOXED_JS_FOR_WEB_TEMPLATE___

const query = require('queryPermission');
const sendPixel = require('sendPixel');
const encodeUriComponent = require('encodeUriComponent');
const campaign = data.campaign.substring(0,24);
const type = data.type === 'custom' ? data.customType : data.type;
const url = 'https://track.intellectadz.com/offer/conversion.asp?cid='+encodeUriComponent(campaign)+'&conversionType='+type+'&key=TRANSACTION_ID';
if (query('send_pixel', url)) {
  sendPixel(url, data.gtmOnSuccess, data.gtmOnFailure);
} else {
  data.gtmOnFailure();
}


